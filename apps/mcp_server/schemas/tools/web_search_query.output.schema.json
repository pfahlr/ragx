{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "web.search.query output",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "results": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "title": {"type": "string", "minLength": 1},
          "url": {"type": "string", "format": "uri"},
          "snippet": {"type": "string"}
        },
        "required": ["title", "url"]
      }
    },
    "metadata": {
      "type": "object",
      "additionalProperties": true
    }
  },
  "required": ["results"]
}
