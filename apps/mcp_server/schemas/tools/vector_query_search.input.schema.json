{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Vector Query Search Input",
  "type": "object",
  "properties": {
    "query": {
      "type": "string",
      "minLength": 1
    },
    "top_k": {
      "type": "integer",
      "minimum": 1,
      "maximum": 10
    }
  },
  "required": ["query", "top_k"],
  "additionalProperties": false
}
