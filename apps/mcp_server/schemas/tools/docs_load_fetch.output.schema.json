{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Docs Load Fetch Output",
  "type": "object",
  "properties": {
    "document": {
      "type": "object",
      "properties": {
        "path": {
          "type": "string"
        },
        "contents": {
          "type": "string"
        }
      },
      "required": ["path", "contents"],
      "additionalProperties": false
    },
    "metadata": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
        },
        "authors": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 1
        },
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": ["title", "authors"],
      "additionalProperties": true
    }
  },
  "required": ["document", "metadata"],
  "additionalProperties": false
}
